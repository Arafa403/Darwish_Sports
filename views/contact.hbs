            .logo {
                font-size: 1.3rem;
            }

            .btn {
                padding: 0.5rem 1rem;
                font-size: 0.8rem;
            }

            .footer {
                padding: 40px 0 20px;
            }

            .footer-container {
                padding: 0 1rem;
            }
        }

        /* contact.css - تصميمات خاصة بصفحة التواصل */

        .contact-hero {
            background: linear-gradient(rgba(45, 80, 22, 0.8), rgba(74, 124, 89, 0.8)),
                url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 400"><rect fill="%234a7c59" width="1200" height="400"/><circle fill="%237fb069" cx="200" cy="100" r="60" opacity="0.6"/><circle fill="%23a4c3a2" cx="800" cy="300" r="80" opacity="0.4"/><path fill="%237fb069" d="M0 300c150-30 300-30 450 0s300 30 450 0s300-30 300 0v100H0z" opacity="0.7"/></svg>');
            background-size: cover;
            background-position: center;
            color: var(--white);
            padding: 150px 0 80px;
            text-align: center;
            position: relative;
        }

        .contact-hero::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: linear-gradient(45deg, transparent 49%, rgba(164, 195, 162, 0.1) 50%, transparent 51%);
            animation: shimmer 3s ease-in-out infinite;
        }

        @keyframes shimmer {

            0%,
            100% {
                opacity: 0;
            }

            50% {
                opacity: 1;
            }
        }

        .hero-container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 2rem;
            position: relative;
            z-index: 2;
        }

        .contact-hero h1 {
            font-size: 3rem;
            margin-bottom: 1rem;
            text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.3);
            animation: fadeInUp 1s ease;
        }

        .contact-hero p {
            font-size: 1.2rem;
            margin-bottom: 0;
            max-width: 600px;
            margin-left: auto;
            margin-right: auto;
            text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.2);
            animation: fadeInUp 1s ease 0.2s both;
        }

        @keyframes fadeInUp {
            from {
                opacity: 0;
                transform: translateY(30px);
            }

            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        .container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 2rem;
        }

        .section-header {
            text-align: center;
            margin-bottom: 3rem;
        }

        .section-header h2 {
            font-size: 2.5rem;
            color: var(--primary-green);
            margin-bottom: 1rem;
            position: relative;
            display: inline-block;
        }

        .section-header h2::after {
            content: '';
            position: absolute;
            bottom: -10px;
            left: 50%;
            transform: translateX(-50%);
            width: 60px;
            height: 4px;
            background: linear-gradient(90deg, var(--secondary-green), var(--light-green));
            border-radius: 2px;
        }

        .section-header p {
            font-size: 1.1rem;
            color: var(--text-medium);
            max-width: 500px;
            margin: 0 auto;
            line-height: 1.6;
        }

        .contact-section {
            padding: 80px 0;
            background: var(--light-gray);
        }

        .contact-content {
            display: flex;
            justify-content: center;
        }

        .contact-info-container {
            max-width: 600px;
            margin: 0 auto;
        }

        .contact-methods {
            display: flex;
            flex-direction: column;
            gap: 1.5rem;
        }

        .contact-method {
            display: flex;
            align-items: center;
            gap: 1.5rem;
            padding: 1.5rem;
            background: var(--white);
            border-radius: 15px;
            text-decoration: none;
            color: inherit;
            transition: all 0.3s ease;
            box-shadow: 0 5px 20px rgba(0, 0, 0, 0.05);
            border: 2px solid transparent;
        }

        .contact-method:hover {
            transform: translateY(-5px);
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
            border-color: var(--accent-green);
        }

        .method-icon {
            width: 60px;
            height: 60px;
            background: linear-gradient(135deg, var(--secondary-green), var(--light-green));
            color: var(--white);
            border-radius: 15px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.5rem;
            flex-shrink: 0;
            transition: all 0.3s ease;
        }

        .method-icon.whatsapp {
            background: linear-gradient(135deg, #25d366, #20b954);
        }

        .method-icon.facebook {
            background: linear-gradient(135deg, #1877f2, #166fe5);
        }

        .method-icon.location {
            background: linear-gradient(135deg, var(--accent-green), var(--light-green));
        }

        .contact-method:hover .method-icon {
            transform: scale(1.1) rotate(5deg);
        }

        .method-info h3 {
            font-size: 1.3rem;
            color: var(--primary-green);
            margin-bottom: 0.5rem;
            font-weight: 600;
        }

        .method-info p {
            color: var(--text-dark);
            margin-bottom: 0.3rem;
            font-size: 1rem;
            font-weight: 500;
        }

        .method-info span {
            color: var(--text-medium);
            font-size: 0.9rem;
        }

        .faq-section {
            padding: 80px 0;
            background: var(--white);
        }

        .faq-container {
            max-width: 800px;
            margin: 0 auto;
        }

        .faq-item {
            background: var(--white);
            border: 2px solid var(--very-light-green);
            border-radius: 12px;
            margin-bottom: 1rem;
            overflow: hidden;
            transition: all 0.3s ease;
        }

        .faq-item:hover {
            border-color: var(--accent-green);
            box-shadow: 0 5px 20px rgba(0, 0, 0, 0.08);
        }

        .faq-item.active {
            border-color: var(--secondary-green);
            box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);
        }

        .faq-question {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 1.5rem 2rem;
            cursor: pointer;
            background: var(--white);
            transition: all 0.3s ease;
        }

        .faq-question:hover {
            background: var(--very-light-green);
        }

        .faq-item.active .faq-question {
            background: var(--very-light-green);
            border-bottom: 2px solid var(--accent-green);
        }

        .faq-question h3 {
            color: var(--primary-green);
            font-size: 1.2rem;
            font-weight: 600;
            margin: 0;
            flex: 1;
        }

        .faq-question i {
            color: var(--secondary-green);
            font-size: 1.1rem;
            transition: transform 0.3s ease;
        }

        .faq-item.active .faq-question i {
            transform: rotate(180deg);
            color: var(--primary-green);
        }

        .faq-answer {
            max-height: 0;
            overflow: hidden;
            transition: max-height 0.3s ease, padding 0.3s ease;
            background: var(--light-gray);
        }

        .faq-item.active .faq-answer {
            max-height: 200px;
            padding: 1.5rem 2rem;
        }

        .faq-answer p {
            color: var(--text-dark);
            line-height: 1.6;
            margin: 0;
        }

        @media (max-width: 992px) {
            .contact-content {
                justify-content: center;
            }
        }

        @media (max-width: 768px) {
            .contact-hero h1 {
                font-size: 2.5rem;
            }

            .contact-hero p {
                font-size: 1.1rem;
            }

            .section-header h2 {
                font-size: 2.2rem;
            }

            .contact-method {
                flex-direction: column;
                text-align: center;
                gap: 1rem;
                padding: 2rem 1.5rem;
            }

            .method-icon {
                margin: 0 auto;
            }

            .faq-question {
                padding: 1.2rem 1.5rem;
            }

            .faq-answer {
                padding: 1.2rem 1.5rem;
            }

            .faq-item.active .faq-answer {
                padding: 1.2rem 1.5rem;
            }
        }

        @media (max-width: 480px) {
            .container {
                padding: 0 1rem;
            }

            .contact-hero {
                padding: 120px 0 60px;
            }

            .contact-hero h1 {
                font-size: 2rem;
            }

            .contact-hero p {
                font-size: 1rem;
            }

            .contact-section {
                padding: 60px 0;
            }

            .contact-form-container {
                padding: 1.5rem;
            }

            .section-header {
                margin-bottom: 2rem;
            }

            .section-header h2 {
                font-size: 1.8rem;
            }

            .method-info h3 {
                font-size: 1.1rem;
            }

            .method-info p {
                font-size: 0.95rem;
            }

            .faq-section {
                padding: 60px 0;
            }

            .faq-question h3 {
                font-size: 1.1rem;
            }

            .faq-question {
                padding: 1rem;
            }

            .faq-item.active .faq-answer {
                padding: 1rem;
                max-height: 250px;
            }
        }

        .contact-info-container {
            animation: fadeInUp 0.8s ease;
        }

        .contact-method {
            animation: slideInRight 0.6s ease;
        }

        .contact-method:nth-child(2) {
            animation-delay: 0.1s;
        }

        .contact-method:nth-child(3) {
            animation-delay: 0.2s;
        }

        .contact-method:nth-child(4) {
            animation-delay: 0.3s;
        }

        @keyframes slideInRight {
            from {
                opacity: 0;
                transform: translateX(30px);
            }

            to {
                opacity: 1;
                transform: translateX(0);
            }
        }

        .working-hours {
            animation: fadeIn 0.8s ease 0.4s both;
        }

        @keyframes fadeIn {
            from {
                opacity: 0;
            }

            to {
                opacity: 1;
            }
        }

        .faq-item {
            animation: fadeInUp 0.6s ease;
        }


        .faq-item:nth-child(2) {
            animation-delay: 0.1s;
        }

        .faq-item:nth-child(3) {
            animation-delay: 0.2s;
        }

        .faq-item:nth-child(4) {
            animation-delay: 0.3s;
        }

        .faq-item:nth-child(5) {
            animation-delay: 0.4s;
        }
    </style>
</head>

